// Bouton Analyse IA
const analyzeWithAI = async (comboId: number) => {
  try {
    const response = await fetch(`${API_BASE}/api/combos/analyze-ai/${comboId}`, {
      method: 'POST'
    });
    const data = await response.json();
    if (data.analysis) {
      alert(`ðŸ¤– Analyse IA:\n\n${data.analysis}`);
      fetchData(); // Refresh
    } else {
      alert(`âŒ Erreur: ${data.error}`);
    }
  } catch (error) {
    console.error('Erreur analyse IA:', error);
  }
};

